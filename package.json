{
  "name": "vibe-icon",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "setup": "rm -rf dist-icons && tsx scripts/build.ts",
    "build": "rm -rf dist && rm -rf dist-site && tsc && npm run build:lib && npm run build:site && npm run build:types",
    "build:site": "vite build --config vite-site.config.ts",
    "build:lib": "tsc lib/index.ts --outDir dist",
    "build:types": "tsc lib/index.ts --declaration --emitDeclarationOnly --outDir dist",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^24.0.1",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "lit-html": "^3.3.0",
    "rxjs": "^7.8.2",
    "shiki": "^3.6.0"
  }
}